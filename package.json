{
  "name": "@bizongo/go-bits",
  "version": "1.5.35",
  "description": "React-bits is a library of React components which will be used across all Bizongo frontend projects. Live demo of components can be found at [https://Bizongo.github.io/react-bits](https://Bizongo.github.io/react-bits)",
  "main": "index.js",
  "scripts": {
    "start": "yarn storybook",
    "storybook": "start-storybook -s ./src -p 7007",
    "clean": "yarn cache clean && rm -rf node_modules yarn.lock && yarn",
    "lint": "eslint src --fix",
    "test": "yarn run jest --silent",
    "coverage": "yarn run jest --silent --onlyFailures --collectCoverage",
    "link-dev": "cd dist; yarn unlink && yarn link",
    "dev": "mkdir dist; cp package.json dist/ && yarn run link-dev && webpack --watch",
    "pre-build": "rm -rf dist/*",
    "build": "yarn pre-build && webpack --profile --json > stats.json",
    "deploy": "storybook-to-ghpages",
    "size": "webpack-bundle-analyzer stats.json",
    "preversion": "yarn build",
    "prepare-release": "cp package.json .npmrc dist/",
    "postpublish": "cd ../ && yarn deploy",
    "release": "npm version patch && yarn prepare-release && cd dist && npm publish",
    "release:minor": "npm version minor && yarn prepare-release && cd dist && npm publish",
    "release:major": "npm version major && yarn prepare-release && cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bizongo/go-bits.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "shared",
    "components",
    "components"
  ],
  "author": "Swapnil Singh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bizongo/go-bits/issues"
  },
  "homepage": "https://Bizongo.github.io/go-bits",
  "peerDependencies": {
    "date-fns": "^1.30.1",
